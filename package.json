{
  "name": "scl",
  "version": "0.3.0",
  "description": "Foundation for SCL React Projects",
  "main": "src/app.jsx",
  "scripts": {
    "start": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --hot --colors --content-base static/",
    "build": "echo 'File Size (before): ' && ls -lah dist/scl.min.js | awk '{ print $5}' && NODE_ENV=production webpack -p --config webpack.production.config.js --progress --colors",
    "test": "npm run lint",
    "lint": "./node_modules/eslint/bin/eslint.js -c ./.eslintrc --ext .jsx --ext .js src",
    "stats": "webpack --profile --json > stats.json && echo 'Upload here ---> webpack.github.io/analyse/#modules'",
    "clean": "rm -rf node_modules/ && npm i && npm start"
  },
  "author": "justin@secretcowlevel.com",
  "license": "none",
  "dependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^1.2.8",
    "react-redux": "^4.4.1",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "compression-webpack-plugin": "^0.3.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^1.13.2",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.15.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
